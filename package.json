{
  "name": "@deeeep/glean",
  "version": "0.1.0",
  "description": "Harvest knowledge from your AI coding sessions - Claude Code plugin suite",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "glean": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./spaced-repetition": {
      "types": "./lib/spaced-repetition.d.ts",
      "import": "./lib/spaced-repetition.js"
    },
    "./harvest-store": {
      "types": "./lib/harvest-store.d.ts",
      "import": "./lib/harvest-store.js"
    },
    "./insight-store": {
      "types": "./lib/insight-store.d.ts",
      "import": "./lib/insight-store.js"
    },
    "./learn-store": {
      "types": "./lib/learn-store.d.ts",
      "import": "./lib/learn-store.js"
    },
    "./bridge/*": {
      "import": "./lib/bridge/*.js"
    }
  },
  "keywords": [
    "claude",
    "claude-code",
    "plugin",
    "session",
    "knowledge",
    "productivity",
    "ai",
    "coding-assistant"
  ],
  "author": {
    "name": "lledellebell",
    "email": "lledellebell@users.noreply.github.com",
    "url": "https://github.com/lledellebell"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lledellebell/glean.git"
  },
  "bugs": {
    "url": "https://github.com/lledellebell/glean/issues"
  },
  "homepage": "https://github.com/lledellebell/glean#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    ".claude-plugin/",
    "bin/",
    "commands/",
    "agents/",
    "plugins/",
    "lib/",
    "shared/",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "echo 'No linting configured yet'",
    "test": "node --test tests/*.test.js",
    "test:watch": "node --test --watch tests/*.test.js",
    "validate": "node scripts/validate-plugin.js"
  }
}
